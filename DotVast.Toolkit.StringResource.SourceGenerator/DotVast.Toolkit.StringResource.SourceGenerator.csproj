	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.CodeAnalysis.CSharp.Workspaces" Version="4.3.0" PrivateAssets="all" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\DotVast.Toolkit.StringResource.Attributes\DotVast.Toolkit.StringResource.Attributes.csproj" />
	</ItemGroup>

</Project>
